{
  "schemaVersion": 1,
  "id": "${modId}",
  "version": "${modVersion}",
  "name": "${modName}",
  "description": "${modDesc}",
  "authors": [
    ${modAuthors}, ${modCredits}
  ],
  "license": "${modLicense}",
  "icon": "ecr.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      "team._0mods.ecr.ECRManager::init"
    ],
    "client": [
      "team._0mods.ecr.ECRManager::client"
    ],
    "jei_mod_plugin": [ "team._0mods.ecr.common.compact.jei.ECJEIPlugin" ]
  },
  "mixins": [
    "${modId}.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=0.15.11",
    "minecraft": "~${minecraftVersion}",
    "fabric-api": "*",
    "hollowcore": ">=${hcVersion}"
  },
  "accessWidener": "${modId}.accesswidener"
}